# Environment Variables Template
# Copy to .env and fill in your values

# GitHub Configuration
GITHUB_TOKEN=your_github_token_here
GITHUB_APP_ID=your_github_app_id_here
GITHUB_CLIENT_SECRET=your_github_client_secret_here
GITHUB_WEBHOOK_SECRET=your_github_webhook_secret_here

# Google AI Configuration
GOOGLE_PROJECT_ID=your_google_project_id_here
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
GOOGLE_APPLICATION_CREDENTIALS=path/to/service_account.json
GOOGLE_AI_API_KEY=your_google_ai_api_key_here

# Microsoft AI Configuration
AZURE_TENANT_ID=your_azure_tenant_id_here
AZURE_CLIENT_ID=your_azure_client_id_here
AZURE_CLIENT_SECRET=your_azure_client_secret_here
AZURE_SUBSCRIPTION_ID=your_azure_subscription_id_here
OPENAI_API_KEY=your_openai_api_key_here

# Firebase Configuration
FIREBASE_PROJECT_ID=your_firebase_project_id_here
FIREBASE_API_KEY=your_firebase_api_key_here
FIREBASE_SERVICE_ACCOUNT_PATH=path/to/firebase_service_account.json

# Jules Agent Configuration
JULES_API_ENDPOINT=your_jules_api_endpoint_here
JULES_API_KEY=your_jules_api_key_here
JULES_WEBHOOK_SECRET=your_jules_webhook_secret_here

# Database Configuration
POSTGRES_DB=ai_orchestration
POSTGRES_USER=ai_admin
POSTGRES_PASSWORD=ai_password_change_me
DATABASE_URL=postgresql://ai_admin:ai_password_change_me@localhost:5432/ai_orchestration

# Redis Configuration
REDIS_URL=redis://localhost:6379/0
CELERY_BROKER_URL=redis://localhost:6379/0
CELERY_RESULT_BACKEND=redis://localhost:6379/0

# Monitoring Configuration
GRAFANA_USER=admin
GRAFANA_PASSWORD=admin_change_me
PROMETHEUS_URL=http://localhost:9090
GRAFANA_URL=http://localhost:3000

# Development Configuration
AI_ORCHESTRATION_ENV=development
DEBUG=true
LOG_LEVEL=INFO
NODE_ENV=development

# Resource Configuration
MAX_GPU_MEMORY=30gb
MAX_CPU_CORES=8
DEFAULT_MODEL_CACHE_SIZE=10gb
NVIDIA_VISIBLE_DEVICES=all
CUDA_VISIBLE_DEVICES=all

# Security Configuration
SECRET_KEY=your_secret_key_here_change_this_in_production
JWT_SECRET_KEY=your_jwt_secret_key_here_change_this_in_production
ENCRYPTION_KEY=your_encryption_key_here_change_this_in_production

# API Configuration
API_V1_STR=/api/v1
PROJECT_NAME="Multi-Platform AI Orchestration"
BACKEND_CORS_ORIGINS=["http://localhost:3000", "http://localhost:8080"]

# Performance Configuration
MAX_CONCURRENT_TASKS=10
TASK_TIMEOUT_SECONDS=300
MODEL_INFERENCE_TIMEOUT=120
API_RATE_LIMIT=1000

# Feature Flags
ENABLE_GPU_OPTIMIZATION=true
ENABLE_MONITORING=true
ENABLE_AUTO_SCALING=true
ENABLE_DISTRIBUTED_EXECUTION=true